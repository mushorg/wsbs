sub yahoo() {
    my @lst;
    my $key = $_[0];
    my $num   = 100;
    my $max  = 1000;
    for($b=0;$b < 1000; $start += 100){
my $yahoo = ("http://search.yahooapis.com/WebSearchService/V1/webSearch?appid=SiteSearch&query=".key($key)."&results=".$num."&start=".$b);
 my $Res = query($yahoo);
 while ( $Res =~ m/<Url>http:\/\/(.+?)\<\/Url>/g){
     if ( $1 !~ /yahoo/ ) {
     my $k    = "$1/";
     my @grep = links($k);
     push( @lst, @grep );
 }
    }
    return @lst;
}

