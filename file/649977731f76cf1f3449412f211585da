sub search() {
    my @lst;
    my $key = $_[0];
    for ( $b = 1 ; $b <= 100 ; $b++ ) {
        my $sc =
          ( "http://www.search.com/search?q=" . key($key) . "&nav=" . $b );
        my $Res = query($sc);
        while ( $Res =~ m/<a href=\"?http:\/\/(.+?)/g ) {
            my $k    = $1;
            my @grep = links($k);
            push( @lst, @grep );
        }
    }
    return @lst;
}

