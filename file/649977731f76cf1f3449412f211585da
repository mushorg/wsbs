 my $Res = query($UoL);
 while ( $Res =~ m/<a href=\"http:\/\/([^>\"]*)/g ) {
     my $k = $1;
     if ( $k !~ /busca|uol|yahoo/ ) {
  my $k    = $1;
  my @grep = links($k);
  push( @lst, @grep );
     }
 }
    }
    return @lst;
}

