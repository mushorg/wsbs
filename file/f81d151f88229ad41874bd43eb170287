sub end() {
    $string = $_[0];
    $string .= "/";
    $string =~ s/\/\//\//;
    while ( $string =~ /\/\// ) {
 $string =~ s/\/\//\//;
    }
    return ($string);
}

sub query($) {
    my $url = $_[0];
    $url =~ s/http:\/\///;
    my $host  = $url;
    my $query = $url;
    my $page  = "";
    $host  =~ s/href=\"?http:\/\///;
    $host  =~ s/([-a-zA-Z0-9\.]+)\/.*/$1/;
    $query =~ s/$host//;
    if ( $query eq "" ) { $query = "/"; }
    eval {
 my $sock = IO::Socket::INET->new(
     PeerAddr => "$host",
     PeerPort => "80",
     Proto    => "tcp"
 ) or return;
 print $sock
"GET $query HTTP/1.0\r\nHost: $host\r\nAccept: */*\r\nUser-Agent: Mozilla/5.0\r\n\r\n";
 my @r = <$sock>;
 $page = "@r";
 close($sock);
    };
    return $page;
}

sub unici {
    my @unici = ();
    my %visti = ();
    foreach my $elemento (@_) {
 $elemento =~ s/\/+/\//g;
 next if $visti{$elemento}++;
 push @unici, $elemento;
    }
    return @unici;
}
}
}
