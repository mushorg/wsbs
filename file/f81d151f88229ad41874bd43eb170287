           $newspread = $1;
           $spread = $newspread;
&display( $dsp, "PRIVMSG $channel :4(0Spread Change4)0 Done");
    }

 if ( $line =~ /PRIVMSG $channel :!help/ ) {
 sleep(3);
 &display( $dsp, "PRIVMSG $channel :4,105For scan use 4$command");
 &display( $dsp, "PRIVMSG $channel :4,105For info use 4!info");
    }

    if ( $line =~ /PRIVMSG $channel :!mostraid/ ) {
 &display( $dsp, "PRIVMSG $channel :4,1 Id: 05$id");
    }

    if ( $line =~ /PRIVMSG $channel :!mostracmd/ ) {
 &display( $dsp, "PRIVMSG $channel :4,1CMD: 05$irccmd");
    }

