`expr \`cat /var/run/dmesg.boot | grep "real memory" | cut -f5 -d" "\` \/ 1048576`;
            $swap = `$toploc | grep -i swap | cut -f2 -d" " | cut -f1 -d"M"`;
            chomp($memory);
            chomp($swap);
        }

        elsif ( $sysos =~ /linux/i ) {
            $sysname = `hostname -f`;
            $memory  = `free -m |grep -i mem | awk '{print \$2}'`;
            $swap    = `free -m |grep -i swap | awk '{print \$2}'`;
            chomp($swap);
            chomp($memory);
        }
